<div class = container>



  <h2>Create a Gig</h2>



  <%= simple_form_for @gig do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
  <%= f.input :price %>
  <%= f.input :seating_capacity %>
  <%= f.input :description %>
  <%= f.input :date, as: :date, start_year: Date.today.year,
  end_year: Date.today.year + 10, discard_day: false,
  order: [:day, :month, :year] %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit %>
  <% end %>

</div>
